public class EvenNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//WAP print even number between 1 to 100
		
		System.out.println("Even numbers: ");
        for (int i = 0; i <= 100; i += 2) 
        {
            System.out.print(i + " ");
        }
        {
        	for (int j = 0; j <= 10; j++);
        
        }
        
        
        System.out.println();

	}

}

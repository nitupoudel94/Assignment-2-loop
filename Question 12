public class ABCDE {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// WAP to print A to z 
		char ch = 'A';
		for ( int i = 1; i <= 6 ; i++)
			
		{
			for (int j = 1 ; j<=5 ; j++)
			{
			System.out.print( ch + "  " );
	ch++;
		}
			
			System.out.println();
		}


	}

}

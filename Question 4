public class leftangletriangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//WAP to print left angled triangle star pattern

		int rows = 5;
		for (int i=1; i<=rows; i++)
{
	for (int k=rows; k>=i; k--)
	{
		System.out.print("*");
	}
	for (int j=1; j<i; j++)
	{
		System.out.print(" ");
	}
	
	System.out.println();
}
		

}
}

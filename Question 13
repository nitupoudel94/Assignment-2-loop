ublic class OddNumbers 
{

	public static void main(String[] args)
	{
		// TODO Auto-generated method stub
		//WAP print odd number between 1 to 100
		
		 System.out.println("Odd numbers: ");
	        for (int i = 1; i <= 100; i += 2) 
	        {
	            System.out.print(i + " ");
	        }
	        {
	        	for (int j = 0; j <= 10; j++);
	        
	        }
	        
	        
	        System.out.println();

	}

}

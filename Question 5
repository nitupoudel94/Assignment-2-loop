public class rightangledtriangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//right angled triangle star pattern
		int n = 5;
		for (int i=0; i<n; i++)
		{
			for (int j=0; j<=i; j++)
			{
				System.out.print("*");
				
			}
			System.out.println();
			}
	}

}

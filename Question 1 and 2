public class starpattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//WAP to print star and A pattern
    
    
		for (int i=0; i<5 ; i++)
		{
			for (int j=0; j<5 ; j++)
			{
				System.out.print("* " );
			}
			System.out.println();
			
		}

		
		for (int i=0; i<4 ; i++)
		{
			for (int j=0; j<5 ; j++)
			{
				System.out.print("A " );
			}
			System.out.println();	
			}
	}

}

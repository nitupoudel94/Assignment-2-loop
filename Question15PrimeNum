public class PrimeNumbers1to100 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//WAP to print all prime numbers between 1 to 100
	
		for(int i=2; i<=100; i++)
		{
			boolean isPrimeNumber = true;
			
			
			for (int j = 2; j<i; j++)
			{
				if(i % j == 0)
				{
					isPrimeNumber = false;
					break;
				}
			}
			if(isPrimeNumber)
			{
				System.out.print(i + "  ");
			}
		}
			

	}

}
